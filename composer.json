{
    "name": "indextank/yii2-plugin-env",
    "description": "PHP DotEnv for Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","dotenv",".env"],
    "license": "MIT",
    "authors": [
        {
            "name": "huige",
            "email": "nginx11724@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "indextank\\dotenv\\": "src/"
        },
        "files": ["src/autoload.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}